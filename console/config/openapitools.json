{
  "$schema": "../node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.14.0",
    "generators": {
      "kubernetes": {
        "generatorName": "typescript-fetch",
        "inputSpec": "https://raw.githubusercontent.com/kubernetes/kubernetes/release-%VERSION%/api/openapi-spec/v3/api__v1_openapi.json",
        "output": "src/shared/types/kubernetes/%VERSION%",
        "templateDir": "config/openapi-generator/templates/typescript-fetch",
        "skipValidateSpec": true,
        "additionalProperties": {
          "withoutRuntimeChecks": true
        }
      },
      "openshift": {
        "generatorName": "typescript-fetch",
        "inputSpec": "%OPENSHIFT_OAS_FILE%",
        "output": "src/shared/types/openshift/%VERSION%",
        "templateDir": "config/openapi-generator/templates/typescript-fetch",
        "skipValidateSpec": true,
        "additionalProperties": {
          "withoutRuntimeChecks": true,
          "stringEnums": true
        }
      }
    }
  }
}
